public class A1_Q1_Main {
public static int linearSearch(int [] arr,int size,int key) {
	for (int i = 0; i < arr.length; i++) {
		if(arr[i]==key) {
			return 1;
		}
	}
	return -1;
}
	public static void main(String[] args) {
		int arr[]= {4,7,1,34,46,23};
	}
}
