
public class A1_Q6_Main {
	public static void main(String[] args) {
		int[] arr = { 10, 20, 15, 3, 4, 4, 1 };
		// rank of 10 = 5
		int ele;
		System.out.print("Enter Element from array = ");
		ele = sc.nextInt();
		int ele = 10;
		int rank = 0;
		for (int i = 0; i < arr.length; i++) {

			if (arr[i] <= ele)
