public class A1_Q1_Main {
	public static int linearSearch(int[] arr, int size, int key) {
		int count = 0;
		for (int i = 0; i < arr.length; i++) {
			count++;
			if (arr[i] == key) {

				return count;
			}
		}
		return count;
	}

	public static int binearSearch(int[] arr, int size, int key) {
		int count = 0;
		int left = 0;
		int right = size - 1, mid;
		while(left<=right) {
			mid = left+right/2;
		}
	}

	public static void main(String[] args) {
		int arr[] = { 4, 7, 1, 34, 46, 23 };
		int count = linearSearch(arr, arr.length, 100);

		System.out.println("Count of linear search comparison = " + count);

	}
}
