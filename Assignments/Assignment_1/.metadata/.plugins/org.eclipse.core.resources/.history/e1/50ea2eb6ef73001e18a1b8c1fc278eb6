
public class A1_Q3_Main {
	public static int linearSearch(int[] arr, int size, int key) {
		int count = 0;
		for (int i = 0; i < arr.length; i++) {
			count++;

			if (arr[i] == key) {
				return count;
			}
		}
		return count;
	}
	
